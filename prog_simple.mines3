// comment

inline fn move_right() {
  __code__("^D");
}
inline fn move_up() {
  __code__("^W");
}
inline fn move_left() {
  __code__("^A");
}
inline fn move_down() {
  __code__("^S");
}
inline fn stop() {
  __code__("#E");
}
inline fn look_right() {
  __code__("[D]");
}
inline fn check_empty() {
  __code__("=e");
}

fn main() {
  loop {
    look_right();
    if !check_empty() {
      stop();
    }
    move_right();
  }
  move_left();
  stop();
}

// vim: syntax=rust:
